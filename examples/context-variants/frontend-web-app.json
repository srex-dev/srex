{
  "service_name": "frontend-web-app",
  "description": "Renders user interface and interacts with backend services",
  "metrics": [
    {
      "name": "page_load_time_seconds",
      "type": "latency",
      "source": "grafana"
    },
    {
      "name": "frontend_http_errors_total",
      "type": "error",
      "source": "datadog"
    },
    {
      "name": "user_interactions_total",
      "type": "throughput",
      "source": "newrelic"
    },
    {
      "name": "frontend_memory_usage",
      "type": "utilization",
      "source": "datadog"
    },
    {
      "name": "cdn_cache_hit_rate",
      "type": "availability",
      "source": "prometheus"
    }
  ]
}